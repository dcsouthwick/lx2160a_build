+SRDS_PLL_REF_CLK_SEL_S1=2
diff --git a/lx2160acex7/configs/lx2160a_SD2_4.rcwi b/lx2160acex7/configs/lx2160a_SD2_4.rcwi
new file mode 100644
index 0000000..a0b4cc7
--- /dev/null
+++ b/lx2160acex7/configs/lx2160a_SD2_4.rcwi
@@ -0,0 +1,2 @@
